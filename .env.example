# Example .env file for Qdrant migration
# Copy this to .env and update with your actual values

# AWS Configuration (unchanged)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
# AWS_PROFILE=default

# S3 Storage (unchanged)
APP_S3_BUCKET=your-unique-bucket-name-12345
APP_IMAGES_PREFIX=food-images/
APP_EMBEDDINGS_PREFIX=food-embeddings/

# AI Models (unchanged)
MODEL_ID=amazon.titan-embed-image-v1
OUTPUT_EMBEDDING_LENGTH=1024
CLAUDE_VISION_MODEL_ID=us.anthropic.claude-3-5-sonnet-20241022-v2:0

# Qdrant Configuration (NEW - replace S3 Vectors config)
QDRANT_URL=https://your-cluster.qdrant.tech:6333
QDRANT_API_KEY=your-qdrant-api-key
QDRANT_COLLECTION_NAME=food_embeddings
QDRANT_COLLECTION_NAME_BULK=food_embeddings_bulk
QDRANT_TIMEOUT=60

# Optional
APP_DEBUG=false

# Bulk Ingest
BULK_USER_ID=999999
